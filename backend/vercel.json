{
  "version": 2,
  "builds": [
   {
    "src": "index.js",
    "use": "@vercel/node",
    "config": { "includeFiles": ["dist/**"] }
   }
  ],
  "functions": {
    "cron": {
      "handler": "index.js",
      "schedule": "cron(30 18 * * *)" 
    }
  },
  "routes": [
   {
    "src": "/(.*)",
    "dest": "index.js"
   }
  ]
}
