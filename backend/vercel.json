{
  "version": 2,
  "builds": [
   {
    "src": "  index.js",
    "use": "@vercel/node",
    "config": { "includeFiles": ["dist/**"] }
   }
  ],
  "crons": [
    {
        "path": "/controllers/cron.js",
        "schedule": "30 8 * * *"
    }
],
  "routes": [
   {
    "src": "/(.*)",
    "dest": "index.js"
   }
  ]
}