{
  "version": 2,
  "builds": [
   {
    "src": "  index.js",
    "use": "@vercel/node",
    "config": { "includeFiles": ["dist/**"] }
   }
  ],
  "cron": [
    {
        "path": "/controllers/cron.clearTodaysScore",
        "schedule": "0 0 * * *"
    }
],
  "routes": [
   {
    "src": "/(.*)",
    "dest": "index.js"
   }
  ]
}